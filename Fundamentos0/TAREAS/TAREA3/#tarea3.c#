#include<stdio.h>

int primo(int n);
int leer (int n);
int error (int n);

int main(){
   int n;
   int c, w;

   n=leer(n);
      for(c=0;c<=n;c++){       
            if(primo(c)==1){printf("%d, ",c);}
        }
  if(n<=0){
    w=error(n);
  }

   return 0;
}

int primo(int n){
   int i;
   int condicion;      
      if(n!=1 && n!=0){
         for(i=2;i<=n;i++)       
            if(n%i==0)
               if(n==i) condicion=1;                
               else { 
                  condicion=0;
                 return 0;}
      }
      else condicion=0;
   return condicion;}

int error (int n){
  printf("Ingresa un nÃºmero valido xd");
  return 0;
}

int leer (int n){
printf("Por favor ingrese el numero: ");
  scanf("%i",&n);
  return n;
}
